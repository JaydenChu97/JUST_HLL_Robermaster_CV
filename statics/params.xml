<?xml version="1.0" encoding="UTF-8"?>

<!--> 程序运行静态参数设定 </!-->
<opencv_storage>
    <!--灯柱筛选条件-->
    <armour_detector>
        <angleRange> 20 </angleRange>
        <minArea> 0 </minArea>
        <maxHeightWidthRat> 15 </maxHeightWidthRat>
        <minHeightWidthRat> 1 </minHeightWidthRat>
        <inRangePercent> 0.86 </inRangePercent>
        <outRangePercent> 0.1 </outRangePercent>
        <armourPixelAvg> 60 </armourPixelAvg>
    </armour_detector>

    <!--图像hsv三通道阈值参数-->
    <image_preprocessor_threshod>
        160 180 215 255 220 255
    </image_preprocessor_threshod>
</opencv_storage>
